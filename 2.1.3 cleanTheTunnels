void cleanTheTunnels() {
    repeat (10) {
        if (onBeeper()) {
            turnLeft();
            pickBeeper();
            if(!beeperAhead()){
                turnRight();
            }
            while(beeperAhead()){
                moveForward();
                pickBeeper();
            }
            turnAround();
            while (frontIsClear()){
                moveForward();
            }
            turnLeft()
        }if(frontIsClear()){
            moveForward();
        }
    }
}
